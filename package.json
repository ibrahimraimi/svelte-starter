		"url": "https://github.com/ibrahimraimi/svelte-starter"
	},
	"bugs": {
		"url": "https://github.com/ibrahimraimi/svelte-starter"
	},
	"homepage": "https://github.com/ibrahimraimi/svelte-starter",
	"license": "MIT",
	"devDependencies": {
		"@melt-ui/svelte": "^0.83.0",
		"@playwright/test": "^1.48.0",
		"@significa/svelte-ui": "^0.0.34",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/kit": "^2.7.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^8.56.12",
